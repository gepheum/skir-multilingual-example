{
  "name": "multilingual-example-frontend",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && wds --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier \"src/**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.prettierignore --write",
    "format:check": "prettier \"src/**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.prettierignore --check"
  },
  "dependencies": {
    "skir-client": "^1.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.4",
    "eslint": "^10.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rimraf": "^6.1.3",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "diff": "^8.0.3",
    "minimatch": "^10.2.2"
  }
}
