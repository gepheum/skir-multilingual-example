{
  "name": "multilingual-example-frontend",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && wds --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier \"src/**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.prettierignore --write",
    "format:check": "prettier \"src/**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.prettierignore --check"
  },
  "dependencies": {
    "skir-client": "^1.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.4",
    "eslint": "^9.39.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rimraf": "^3.0.2",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.50.1"
  }
}
